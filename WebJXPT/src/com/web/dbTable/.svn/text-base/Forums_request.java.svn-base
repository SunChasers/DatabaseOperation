package com.web.dbTable;

import java.sql.Time;

public class Forums_request {
	private int ID;//帖子回复代理ID
	private String request_content;//回复内容
	private Time request_time;//回复时间
	private String requesttoID;//回复的帖子ID
	/**
	 * 构造函数
	 * @return
	 */
	public Forums_request(){
		
	}
	public int getID() {
		return ID;
	}
	public String getRequest_content() {
		return request_content;
	}
	public Time getRequest_time() {
		return request_time;
	}
	public String getRequesttoID() {
		return requesttoID;
	}
	
}
