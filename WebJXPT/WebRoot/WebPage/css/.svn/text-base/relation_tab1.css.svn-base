}
table.infotable_hr{
		margin:0 auto;
		border-bottom:1px solid #fff;	
	    width:720px;
	}
	
table.infotable_hr th{
		border-right:1px solid #fff;
		border-bottom:2px solid #FFDD75;
		text-align:center;
		background-color:#AEAEAE;
		color:#FFF;
}
	
th{
	height:50px;
	font-weight:normal;
}
table.infotable_hr td{
	    height:50px;
	    text-align:center;
		border-right:1px solid #fff;
		border-top:1px solid #fff;
		padding-left:5px;
		background-color:#EBEBEB;
	}
.textint{
	 background-image:url("WebPage/img/backg.png");
