table.search{

    margin-top:20px;
    margin-bottom:20px;
	width:32%;
	float: right;
}
.searchdiv{
	text-align: center;
}
table{
	font-family:"宋体", sans-serif;
	font-size:13px;
	color:#000;
	word-spacing:1px;
}
table.search input{
	border:1px solid #DEB960;
	background-color:#FFF;
	letter-spacing:2px;
	height:17px;
}

input.input{
	border:1px solid #DEB960;
	background-color:#FFF;
	height:16px;
    font:12px Arial;
}
input.button{
	font-size: 12px;
	text-decoration: none;
	letter-spacing: 2px;
	background-image:  url(../image/button.gif);
	height: 18px;
	border: 1px solid #D18C0C;
	background-repeat: repeat-x;
	font-family: "宋体",sans-serif;
	color: #333333;
	margin-top:2px;
    overflow:visible;
    padding:4px 10px 0 10px;
    line-height:90%;
}
button.button{
	font-size: 12px;
	text-decoration: none;
	letter-spacing: 2px;
	background-image:  url(../image/button.gif);
	height: 18px;
	border: 1px solid #D18C0C;
	background-repeat: repeat-x;
	font-family: "宋体",sans-serif;
	color: #333333;
	margin-top:2px;
    overflow:visible;
    padding:4px 10px 0 10px;
