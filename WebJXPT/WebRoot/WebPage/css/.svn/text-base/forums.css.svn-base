.forums_left {
	width: 23%;
	height: 100%;
	float: left;
	padding: 0px;
	border-right: solid 1px #DCDCDC;
	background:#ffffff;
}

.forums_right {
	width: 76%;
	height: 100%;
	float: left;
	background: #FFFFFF;
}

.forums_select {
	width: 100%;
	padding: 0px;
	fint-size: 18px;
}

.forums_select_ul {
	padding: 0px;
	margin-top: 0px;
	margin-bottom: 0px;
}

.forums_select_ul li {
	width: 100%;
	height: 50px;
	text-align: center;
	line-height: 50px;
	border-bottom: solid 1px #DCDCDC;
	display: inline-block;
}
